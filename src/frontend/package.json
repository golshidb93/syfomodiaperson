  "scripts": {
    "dev": "title modiasyfofront && npm-run-all images static:build:local scripts:watch",
    "build": "npm-run-all images static:build scripts:build",
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill --require ignore-styles  ./test/setup.js \"test/**/*.@(js|jsx)\"",
    "test:watch": "npm run test -- --watch",
    "static:build:local": "title modiasyfofront && node build-html.js html/modiasyfofront.mustache local",
    "static:build": "node build-html.js html/modiasyfofront.mustache prod",
    "scripts:build": "set NODE_ENV=production&&node deploy.js",
    "scripts:watch": "title modiasyfofront && webpack-dev-server --inline  --port 3040",
    "lint": "eslint js/. --ext .js",
    "lint:fix": "eslint js/. --ext .js --fix",
    "images": "cp -R img ../main/webapp"
  },
